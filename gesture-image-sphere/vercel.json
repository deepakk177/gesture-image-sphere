{
    "version": 2,
    "framework": "vite",
    "name": "gesture-image-sphere",
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Permissions-Policy",
                    "value": "camera=*"
                }
            ]
        }
    ]
}